

    <div class="container my-5">
      <div class="row justify-content-center">
       <div class="col-md-10">
        <a class="btn btn-warning" href="/student/create">Add New Student</a>
        <br>
        <br>
            <div class="card" >
                <div class="card body ">
                    <h2 class="text-center my-3">ALL STUDENT DATA</h2>
                    <hr>
                    <table class="table cardesing">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>NAME</th>
                                <th>EMAIL</th>
                                <th>NUMBER</th>
                                <th>PHOTO</th>
                                <th>ACTION</th>
                            </tr>
                        </thead>
                        <tbody>
                        
                            <% students.forEach((element, index) => { %>
                                
                                <tr>
                                    <td><%= index + 1 %></td>
                                    <td><%= element.name %></td>
                                    <td><%= element.email %></td>
                                    <td><%= element.cell %></td>
                                    <td><img style=" width : 50px ; height : 50px ; object-fit: cover;" src="/assets/upload/<%= element.photo %>" alt=""></td>
                                    <td>
                                     <a class="btn btn-primary btn-sm" href="/student/<%= element._id %>"><i class="fa fa-eye" aria-hidden="true"></i></a>
                                     <a class="btn btn-success btn-sm" href="/student/edit/<%= element._id %>"><i class="fa fa-edit" aria-hidden="true"></i></a>
                                     <a class="btn btn-danger btn-sm" href="/student/delete/<%= element._id %>"><i class="fa fa-trash" aria-hidden="true"></i></a>
                                    </td>
                                </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
      </div>
    </div>





